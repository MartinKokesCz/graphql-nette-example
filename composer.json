{
	"name": "martinkokescz/graphql-nette-example",
	"description": "GraphQL API based on Nette, MySQL, Doctrine example",
	"keywords": [
		"graphql",
		"nette",
		"doctrine",
		"mysql",
		"api",
		"docker"
	],
	"type": "project",
	"license": [
		"MIT",
		"BSD-3-Clause",
		"GPL-2.0-only",
		"GPL-3.0-only"
	],
	"require": {
		"php": ">= 8.3",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/http": "^3.3",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"webonyx/graphql-php": "^15.18",
		"nettrine/annotations": "^0.8",
		"nettrine/orm": "dev-master",
		"nettrine/dbal": "dev-master",
		"nettrine/cache": "^0.4",
		"nettrine/migrations": "^0.9",
		"contributte/console": "^0.10.1"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3",
		"slevomat/coding-standard": "^8.4",
		"phpstan/phpstan": "^1.0",
		"phpstan/phpstan-doctrine": "^1.5",
		"phpstan/phpstan-deprecation-rules": "^1.0",
		"phpstan/phpstan-nette": "^1.0",
		"phpstan/phpstan-strict-rules": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
